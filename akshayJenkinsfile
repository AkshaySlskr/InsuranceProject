pipeline {
  agent any

  tools {
    maven 'M2_HOME'
        }
   
  stages {
    stage('CheckOut') {
      steps {
        echo 'Checkout the source code from GitHub'
        git branch: 'main', url: 'https://github.com/AkshaySlskr/InsuranceProject.git'
            }
        }
  stage('Package') {
      steps {
        echo 'Generate a jar file for the package using Maven'
        sh 'mvn clean package'
            }
        }
    }
}






